{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/monorepo-practice/src",
  "monorepo": true,
  "root": "apps/monorepo-practice",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/monorepo-practice/tsconfig.app.json"
  },
  "projects": {
    "monorepo-practice": {
      "type": "application",
      "root": "apps/monorepo-practice",
      "entryFile": "main",
      "sourceRoot": "apps/monorepo-practice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepo-practice/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}